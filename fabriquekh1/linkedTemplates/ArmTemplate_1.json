						"          date_fin_week),",
						"     pivotBy(indicator),",
						"     count = sum(value),",
						"     columnNaming: 'compancy$N_$V',",
						"     lateral: true) ~> pivot1",
						"pivot1 sort(asc(country, true)) ~> sort1",
