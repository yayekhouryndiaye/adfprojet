				},
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DS_SRC_deathcases",
								"type": "DatasetReference"
							},
							"name": "casesdeaths"
						},
						{
							"dataset": {
								"referenceName": "DS_lookup_country",
								"type": "DatasetReference"
							},
							"name": "lookupcountry"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "DS_deathcases",
								"type": "DatasetReference"
							},
