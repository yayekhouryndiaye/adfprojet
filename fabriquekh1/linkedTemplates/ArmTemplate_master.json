{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nom de fabrique de données",
			"defaultValue": "fabriquekh1"
		},
		"ls_datalakegen2_accountKey": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « accountKey » de « ls_datalakegen2 »"
		},
		"ls_storagebob_connectionString": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « connectionString » de « ls_storagebob »"
		},
		"sl_sqlbd_connectionString": {
			"type": "secureString",
			"metadata": "Chaîne sécurisée pour « connectionString » de « sl_sqlbd »"
		},
		"ls_HttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_datalakegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covsreportingsdl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI pour le compte de stockage contenant des modèles ARM liés"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Jeton SAS pour le compte de stockage contenant des modèles ARM liés"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fabriquekh1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_datalakegen2_accountKey": {
						"value": "[parameters('ls_datalakegen2_accountKey')]"
					},
					"ls_storagebob_connectionString": {
						"value": "[parameters('ls_storagebob_connectionString')]"
					},
					"sl_sqlbd_connectionString": {
						"value": "[parameters('sl_sqlbd_connectionString')]"
					},
					"ls_HttpServer_properties_typeProperties_url": {
						"value": "[parameters('ls_HttpServer_properties_typeProperties_url')]"
					},
					"ls_datalakegen2_properties_typeProperties_url": {
						"value": "[parameters('ls_datalakegen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "fabriquekh1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'fabriquekh1_ArmTemplate_0')]"
			]
		}
	]
}